.footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  display: flex;  
  background: #fff;
  z-index: 9;
}
.footer > view{
  flex: 1;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 16rpx;  
  color: #CECECE;
}
.user_center image{
  width: 46rpx;
  height: 53rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}
.create_code image{
  width: 56rpx;
  height: 56rpx;
  margin-top: 10rpx;
  margin-bottom: 7rpx;
}
.code_list image{
  width: 44rpx;
  height: 45rpx;
  margin-top: 10rpx;
  margin-bottom: 18rpx;
}
.active{
  color: #3A3A3A;
}